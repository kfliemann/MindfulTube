{
    "manifest_version": 2,
    "name": "Tempname",
    "version": "1.0",
  
    "description": "Adds a temp description",
  
    "icons": {
      "48": "icons/tempname-48.png"
    },
  
    "content_scripts": [
      {
        "matches": [
            "*://*.youtube.com/*",
            "*://*.youtube.de/*"
        ],
        "css": ["styles.css"],
        "js": ["tempname.js"],
        "run_at": "document_start"
      }
    ]
  }
  